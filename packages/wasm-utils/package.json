{
  "author": "Webb Developers",
  "name": "@webb-tools/wasm-utils",
  "version": "0.1.4-42",
  "scripts": {
    "build": "rimraf build pkg && yarn build:web && yarn build:bundler && yarn build:nodejs",
    "build:web": "wasm-pack build --target web --out-dir build/web",
    "build:bundler": "wasm-pack build --target bundler --out-dir build/bundler",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir build/njs",
    "test": "wasm-pack test --release --node"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "web/wasm-utils_bg.wasm",
    "web/wasm-utils.js",
    "web/wasm-utils_bg.js",
    "web/wasm-utils.d.ts",
    "bundler/wasm-utils_bg.wasm",
    "bundler/wasm-utils.js",
    "bundler/wasm-utils_bg.js",
    "bundler/wasm-utils.d.ts",
    "njs/wasm-utils_bg.wasm",
    "njs/wasm-utils.js",
    "njs/wasm-utils_bg.js",
    "njs/wasm-utils.d.ts"
  ],
  "module": "wasm-utils.js",
  "types": "wasm-utils.d.ts",
  "devDependencies": {
    "@polkadot/dev": "^0.65.90",
    "@wasm-tool/wasm-pack-plugin": "^1.3",
    "copy-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  }
}
